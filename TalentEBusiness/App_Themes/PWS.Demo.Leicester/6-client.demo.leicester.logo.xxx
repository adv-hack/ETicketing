 .ebiz-logo img {
	height: 4.000rem;
	margin-bottom: 0.625rem;
	margin-top: 0.625rem;
	width: 4.000em; }

/* medium screens */
@media only screen and (min-width: 40.063em) { 
    .ebiz-logo img {
		height: 5.000rem;

nav ul, nav ol {
    list-style: none outside none;
}
a {
    text-decoration: none;
}
.main-header {
    background-color: rgb(41, 38, 39) !important;
    color: rgb(255, 255, 255);
    padding: 8px 0;
    width: 100%;
}
.section-title {
    margin: 0 auto;
    text-align: center;
    width: 940px;
}
.header-left {
	display: block;
    float: left;
    font-family: 'Futura W02';
    font-size: 15px;
    font-weight: bold;
    min-width: 33%;
    text-align: center;
    text-transform: uppercase;
}
.header-event-date {
    display: block;
    font-size: 24px;
    margin: 38px auto 0;
    width: 100%;
}
.header-middle {
    display: block;
    float: left;
    min-width: 34%;
    text-align: center;
}
.header-middle img {
    display: block;
    height: 80px;
    margin: 5px auto;
    width: auto;
}
.header-right {
    display: block;
    float: left;
    min-width: 33%;
    text-align: right;
}
#primary, #secondary {
    background-color: rgb(180, 182, 183) !important;
    border-bottom: 1px solid rgb(255, 255, 255);
    box-sizing: border-box;
    font-family: 'L Futura W01';
    font-style: normal;
    font-weight: 200;
    height: 36px;
    line-height: 31px;
    margin: 0;
    padding: 0;
    text-transform: uppercase;
}
.cat-item a {
    color: #ffffff;
}

@media only screen and (max-width: 40em) { 
	.section-title {
		width: 100%;
	}
	.header-left {
	    display: none;
	}
	.header-middle {
	    box-sizing: border-box;
	    display: block;
	    float: none;
	    margin: 0;
	    padding: 0;
	    text-align: center;
	    width: 100%;
	}
	.header-middle img {
	    height: 40px;
	}
	.header-right {
	    display: none;
	}
} 

@media only screen and (min-width: 40.063em) and (max-width: 64em) { 
	.section-title {
		width: 100%;
	}
	.header-event-date {
		display: block;
    	font-size: 20px;
    	margin-top: 42px !important;
	}
}


nav.g1 { 
    background: none;
    height: 24px;
    position: absolute;
    right: 0;
    top: 0;
    width: auto; }

/* --------------------------------------------------------------------------------------------- footer ----- */
#footer-main {
    background-color: rgb(204,204,204);
    padding: 24px 0;
    color: rgb(255,255,255);
    font-size: 11px;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    /* border-top: 6px solid rgb(58,98,163); */
    margin-top: 48px;
    background-color: rgb(20,20,20);
}
.footer-sitemap {
    width: 940px;
    margin: 0 auto;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
#global-footer-wrapper {
    background-color: rgb(0,0,0);
    padding: 8px 0 12px;
    color: rgb(255,255,255);
    font-size: 10px;
    width: 100%;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
#global-footer {
    width: 940px;
    margin: 0 auto;
    padding: 0;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
#global-footer nav {
    width: 100%;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
#global-footer nav ul {
    width: 100%;
    margin: 0 auto;
    padding: 0;
    list-style: none;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
#global-footer nav ul li {
    float: left;
    display: inline-block;
    color: rgb(255,255,255);
    margin: 0 16px 0 0;
    padding: 0;
    line-height: 30px;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
#global-footer nav ul li a {
    color: rgb(255,255,255);
    text-decoration: none;
    text-transform: uppercase;
}
#global-footer nav ul li a:hover {
    text-decoration: underline !important;
}
.footer-copyright {
    width: 100%;
    clear: both;
    padding-bottom: 12px;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    color: rgb(102,102,102);
}
#footer-goodwoood-logo {
    height: 87px;
}
#footer-goodwoood-logo img {
    width: auto;
    height: 76px;
    margin: 0 auto 40px;
    display: block;
}
.sitemap-goodwood-links {
    text-align: center;
}
#footer-sitemap-nav {
    width: 940px;
    margin: 0 auto;
    padding: 0;
    color: rgb(102,102,102);
}
#footer-sitemap-nav .sitemap-links {
    display: inline-block;
    vertical-align: text-top;
    margin: 0;
    padding: 0;
    text-align: left;
    width: 25%;
    float: left;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
#footer-sitemap-nav ul.sitemap-section {
    margin: 0 0 29px;
    padding: 0 20px;
    list-style: none;
}
#footer-sitemap-nav ul.sitemap-section li a {
    /* color: rgb(102,102,102); */
    color: rgb(255,255,255);
    display: block;
    margin: 0;
    padding: 0 8px;
    line-height: 22px;
    /* border-bottom: 1px solid rgba(0,0,0,0.05); */
    /* border-bottom: 1px solid rgba(255,255,255,0.1); */
    font-weight: normal;
}
#footer-sitemap-nav ul.sitemap-section li a:hover {
    /* background-color: rgba(0,0,0,0.1); */
    text-decoration: underline !important;
}
#footer-sitemap-nav ul.sitemap-section .sitemap-section-title a {
    font-weight: bold;
    text-decoration: none;
    text-transform: uppercase;
}
#footer-sitemap-nav ul.sitemap-section ul {
    margin: 0;
    padding: 0;
    list-style: none;
    display: none;
}
#footer-sitemap-nav ul.sitemap-section ul li a {
    border-bottom: 1px solid rgba(0,0,0,0.05);
    font-weight: normal;
}
.footer-copyright span {
    display: inline;
    float: left;
}

/* User Defined Block Grid */
.ebiz-ud-xlarge-block-grid {
    margin-left: auto;
    margin-right: auto;
    padding: 1.1111rem;
}
.ebiz-ud-xlarge-block-grid li {
    position:relative;
}
.ebiz-ud-xlarge-block-grid a {
    color: #ffffff;
    text-decoration: none;
    text-transform: uppercase;
}


/* Breadcrumb Trail */
body.Home .ebiz-breadcrumb-trail, body.UsrDef .ebiz-breadcrumb-trail {
    display: none;
}

footer ul {
    font-size: 11px; }

@media only screen and (max-width: 40em) {
    
}

@media only screen and (max-width: 64em) {
    .ebiz-ud-xlarge-block-grid .ebiz-ud-label-outer-wrap {
        display: table; 
        position: absolute;
        top: 0;
    }
    .ebiz-ud-xlarge-block-grid .ebiz-ud-label-inner-wrap {
        /*  Chrome 1-9 and Safari 4-5.0  */
        background-image: -webkit-gradient(linear, left top, left bottom, color-stop( 0.0, rgba(0, 0, 0, 0.000) ), color-stop( 1.0, rgba(0, 0, 0, 1.000) ));
        /*  Chrome 10+ Safari 5.1+ Mobile Safari  */
        background-image: -webkit-linear-gradient(top, rgba(0, 0, 0, 0.000), rgba(0, 0, 0, 1.000) );
        /*  Firefox 3.6+  */
        background-image: -moz-linear-gradient(top, rgba(0, 0, 0, 0.000), rgba(0, 0, 0, 1.000) );
        /*  IE 10+  */
        background-image: -ms-linear-gradient(top, rgba(0, 0, 0, 0.000), rgba(0, 0, 0, 1.000) );
        /*  Opera 11.10+  */
        background-image: -o-linear-gradient(top, rgba(0, 0, 0, 0.000), rgba(0, 0, 0, 1.000) );
        /*  W3C  */
        background-image: linear-gradient(top, rgba(0, 0, 0, 0.000), rgba(0, 0, 0, 1.000) );
        display: table-cell; 
        text-align: center;
        vertical-align: bottom; 
    }
}

@media only screen and (min-width: 64.063em) {
    .ebiz-ud-xlarge-block-grid .ebiz-ud-label-outer-wrap {
        display: table; 
        position: absolute;
        top: 0;
    }
    .ebiz-ud-xlarge-block-grid .ebiz-ud-label-inner-wrap {
        background: rgba(0, 0, 0, 0.7);
        display: table-cell; 
        text-align: center;
        opacity: 0;
        transition: opacity 0.5s;
        transition-delay: 0.2s !important;
        vertical-align: middle; 
    }
    .ebiz-ud-xlarge-block-grid .ebiz-ud-label-inner-wrap:hover {
        opacity: 1;
    }
    .ebiz-ud-xlarge-block-grid p {
        line-height: 1.500rem;
        margin-top: 1.500rem;
        font-size: 1.500rem;
        padding-left: 1.250em;
        padding-right: 1.250em;
    }
}

@media only screen and (min-width: 90.063em) { 
    /* User Defined Block Grid */
    .ebiz-ud-xlarge-block-grid {
        max-width: 1656px;
    }
} /* min-width 1441px, xlarge screens */
<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpProtocol>
            <customHeaders>
                <remove name="Pragma" />
                <remove name="Expires" />
                <remove name="Cache-Control" />
            </customHeaders>
        </httpProtocol>
        <staticContent>
            <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="60.00:00:00" />
        </staticContent>
    </system.webServer>
</configuration>

